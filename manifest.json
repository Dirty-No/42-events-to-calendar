{
	"name": "42 events to Google Calendar",
	"description": "Allows you to add 42 events to your Google Calendar conveniently.",
	"version": "1.0",
	"manifest_version": 3,

	"permissions": ["storage", "declarativeContent", "activeTab", "scripting"],

	"content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ],

	"icons": {
		"16": "/images/get_started16.png",
		"32": "/images/get_started32.png",
		"48": "/images/get_started48.png",
		"128": "/images/get_started128.png"
	  }
  }